{
  "name": "voice-orchestrator-api",
  "version": "1.0.0",
  "description": "ADHD-friendly multi-agent assistant with Gmail and Calendar integration via Composio MCP",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev --respawn --transpile-only src/simple-orchestrator.ts",
    "start": "node dist/server.js",
    "start:agents": "ts-node start-agents.ts",
    "start:orchestrator": "ts-node orchestrator-server.ts",
    "start:gmail": "ts-node agents/gmail-agent.ts",
    "start:calendar": "ts-node agents/calendar-agent.ts",
    "start:simple": "ts-node src/simple-orchestrator.ts",
    "health": "curl http://localhost:3000/health",
    "test": "jest",
    "lint": "eslint **/*.ts",
    "setup": "npm install --dangerously-skip-permissions && npm run build"
  },
  "dependencies": {
    "@anthropic-ai/claude-code": "^1.0.71",
    "@composio/core": "^0.2.0-next.7",
    "@modelcontextprotocol/sdk": "^1.0.0",
    "@sentry/node": "^7.0.0",
    "@types/dotenv": "^6.1.1",
    "@types/node": "^20.0.0",
    "axios": "^1.11.0",
    "bull": "^4.12.0",
    "composio-core": "^0.5.39",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "node-fetch": "^3.3.2",
    "ts-node": "^10.9.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/node-fetch": "^2.6.12",
    "@types/uuid": "^9.0.7",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "voice-assistant",
    "multi-agent",
    "orchestrator",
    "api",
    "typescript"
  ],
  "license": "MIT"
}
